<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="HoneycombButton">
        <attr name="hcb_text" format="reference" />
        <attr name="hcb_color" format="integer" />
        <attr name="hcb_borderColor" format="integer" />
        <attr name="hcb_textColor" format="integer" />
        <attr name="hcb_textSize" format="float" />
        <attr name="hcb_cornerRadius" format="float" />
        <attr name="hcb_radius" format="float" />
    </declare-styleable>

    <declare-styleable name="HoneycombLayout">
        <attr name="hcl_amount" format="integer" />
        <attr name="hcl_radius" format="float" />
    </declare-styleable>
</resources>